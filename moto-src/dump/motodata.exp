CREATE FUNCTION moto_mktg_allrec.run_moto_mktg_incr() RETURNS void
    LANGUAGE plpgsql
    AS $$  -- executes an incremental load for the moto_mktg_allrec source
declare
	rec record;
begin
 
	for rec in select table_name from information_schema.tables where table_schema = 'moto_mktg' and table_name not like 'jrn_%'
	loop
	  execute concat('TRUNCATE TABLE moto_mktg_allrec.jrn_',rec.table_name,';');
	  execute concat('moto_mktg_allrec.cdc_jrn_', rec.table_name, '();');
	end loop;

end;

$$;


