-- Name: print_nullable(anyelement); Type: FUNCTION; Schema: vaultspeed_proc; Owner: amata
--

CREATE FUNCTION vaultspeed_proc.print_nullable(value anyelement) RETURNS text
    LANGUAGE plpgsql
    AS $$
-- Function which returns the input as a quoted string if the value is not null and the string 'null' otherwise.
begin
	return coalesce(quote_literal(value), 'null');
end;
$$;


