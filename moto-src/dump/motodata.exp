--
-- Name: view_contact_emails; Type: VIEW; Schema: moto_sales_files; Owner: amata
--

CREATE VIEW moto_sales_files.view_contact_emails AS
 SELECT pty.party_number AS contact_id,
    ema.name AS email_address,
    con.contact_id AS real_contact_id
   FROM (((moto_mktg.party pty
     JOIN moto_mktg.party_contacts pco ON ((pty.party_number = pco.party_number)))
     JOIN moto_mktg.contacts con ON ((con.contact_id = pco.contact_id)))
     JOIN moto_mktg.e_mails ema ON ((con.contact_id = ema.contact_id)));


ALTER TABLE moto_sales_files.view_contact_emails OWNER TO amata;

--
-- Name: view_contact_phones; Type: VIEW; Schema: moto_sales_files; Owner: amata
--

CREATE VIEW moto_sales_files.view_contact_phones AS
 SELECT pty.party_number AS contact_id,
    phn.phone_number,
    con.contact_id AS real_contact_id
   FROM (((moto_mktg.party pty
     JOIN moto_mktg.party_contacts pco ON ((pty.party_number = pco.party_number)))
     JOIN moto_mktg.contacts con ON ((con.contact_id = pco.contact_id)))
     JOIN moto_mktg.phones phn ON ((con.contact_id = phn.contact_id)));


ALTER TABLE moto_sales_files.view_contact_phones OWNER TO amata;

--
-- Name: view_customer_parties; Type: VIEW; Schema: moto_sales_files; Owner: amata
--

CREATE VIEW moto_sales_files.view_customer_parties AS
 SELECT c.party_number,
    c.parent_party_number,
    c.name,
    c.birthdate,
    c.gender,
    c.party_type_code,
    c.comments,
    c.address_number,
    c.update_user,
    u.national_person_id
   FROM (moto_mktg.party c
     JOIN moto_sales.customers u ON ((u.customer_number = c.party_number)))
  ORDER BY u.customer_number;


ALTER TABLE moto_sales_files.view_customer_parties OWNER TO amata;

