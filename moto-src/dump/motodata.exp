--
-- Name: create_jrn_func(character varying); Type: FUNCTION; Schema: moto_mktg; Owner: amata
--

CREATE FUNCTION moto_mktg.create_jrn_func(ptable_name character varying) RETURNS text
    LANGUAGE plpgsql
    AS $_$

declare
 
	cols				varchar[];
	keys				varchar[];
	old_case_cols		varchar[];
	new_case_cols		varchar[];
  	attribute_cols		varchar[];
 
begin
	
	select
		array_agg(column_name::varchar)
	into cols
	from information_schema.columns
	where table_schema = 'moto_mktg'
		and table_name = ptable_name;
	
	select
		array_agg(c.column_name::varchar)
	into keys
	from information_schema.table_constraints          tc
	join information_schema.constraint_column_usage as ccu using (constraint_schema, constraint_name)
	join information_schema.columns                 as c
	     on c.table_schema = tc.constraint_schema and tc.table_name = c.table_name and ccu.column_name = c.column_name
	where constraint_type = 'PRIMARY KEY'
		and tc.table_name = ptable_name
		and tc.table_schema = 'moto_mktg';
	
	if keys is null then
		select
			array_agg(c.column_name::varchar)
		into keys
		from information_schema.table_constraints          tc
		join information_schema.constraint_column_usage as ccu using (constraint_schema, constraint_name)
		join information_schema.columns                 as c
		     on c.table_schema = tc.constraint_schema and tc.table_name = c.table_name and ccu.column_name = c.column_name
		where constraint_type = 'UNIQUE'
			and tc.table_name = ptable_name
			and tc.table_schema = 'moto_mktg';
	end if;
	
	keys := array_append(keys, 'update_timestamp');
  
  select array_agg(element), array_agg(old_cc), array_agg(new_cc) into attribute_cols, old_case_cols, new_case_cols
  from(
	select 	concat('CASE WHEN NEW.',element,' = OLD.',element,'
		                 THEN NULL
		                 ELSE OLD.',element,'
                 END as ',element) as old_cc,
			concat('CASE WHEN NEW.',element,' = OLD.',element,'
		                 THEN NULL
		                 ELSE NEW.',element,'
                 END as ',element) as new_cc,
			element
	from (
	  select unnest(cols)
	  except
	  select unnest(keys)
	) t (element))c;

	return concat('
		create or replace function moto_mktg.cdc_',ptable_name,'(
		) returns trigger as
		$$
		
		declare
		
		    vtrans_id int = nextval(''moto_mktg.transaction_seq'');
		
		begin
			--
			-- Create a row in jrn_',ptable_name,' to reflect the operation performed on ',ptable_name,',
			-- make use of the special variable TG_OP to work out the operation.
			--
			if (tg_op = ''DELETE'') then
		            INSERT INTO moto_mktg.jrn_',ptable_name,'
		                        (
		                         trans_id
		                        ,image_type
		                        ,operation
		                        ,trans_timestamp
		                        ,',array_to_string(cols, ', '),'
		                         )
		                  SELECT
		                         vtrans_id
		                         ,NULL
		                         ,''D''
		                         ,clock_timestamp()
		                         ,old.',array_to_string(cols, ', old.'),';
				return old;
		    elsif (tg_op = ''UPDATE'') then
		            INSERT INTO moto_mktg.jrn_',ptable_name,'
		                        (
		                        trans_id
		                        ,image_type
		                        ,operation
		                        ,trans_timestamp
		                        ,',array_to_string(keys, ', '),'
		                        ',','||array_to_string(attribute_cols, ', '),'
		                        )
		                    SELECT vtrans_id
		                         ,''BEFORE''
		                         ,''U''
		                         ,clock_timestamp()
		                         ,OLD.',array_to_string(keys, ', OLD.'),'
		                         ',','||array_to_string(old_case_cols, ', '),'
		                UNION ALL
		                  SELECT vtrans_id
		                         ,''AFTER''
		                         ,''U''
		                         ,clock_timestamp()
		                         ,NEW.',array_to_string(keys, ', NEW.'),'
		                         ',','||array_to_string(new_case_cols, ', '),'
		                         ;
				return new;
			elsif (tg_op = ''INSERT'') then
		            INSERT INTO moto_mktg.jrn_',ptable_name,'
		                (
		                trans_id
		                ,image_type
		                ,operation
		                ,trans_timestamp
		                ,',array_to_string(cols, ', '),'
		                )
				        SELECT
	                  vtrans_id
			              , NULL
			              , ''I''
			              ,clock_timestamp()
			              ,new.',array_to_string(cols, ', new.'),';
				return new;
			end if;
			return null; -- result is ignored since this is an AFTER trigger
		end;
		$$
			language plpgsql;
	');
 
end;
$_$;


ALTER FUNCTION moto_mktg.create_jrn_func(ptable_name character varying) OWNER TO amata;

--
-- Name: build_moto_mktg_schema(); Type: FUNCTION; Schema: moto_mktg_allrec; Owner: amata
--

CREATE FUNCTION moto_mktg_allrec.build_moto_mktg_schema() RETURNS void
    LANGUAGE plpgsql
    AS $$  -- executes the DDL to create the objects in moto_mktg_allrec and creates functions for copying the cdc info from moto_mktg
declare
	rec record;
  rec_fk record;
begin
	for rec in select table_name from information_schema.tables where table_schema = 'moto_mktg' and table_name not like 'jrn_%'
	loop
	  execute concat('DROP TABLE IF EXISTS moto_mktg_allrec.',rec.table_name,' CASCADE;');
	  execute concat('DROP TABLE IF EXISTS moto_mktg_allrec.jrn_',rec.table_name,' CASCADE;');
	  execute concat('create table moto_mktg_allrec.',rec.table_name,' (like moto_mktg.',rec.table_name,' INCLUDING ALL);');
	  execute concat(
	    'create table moto_mktg_allrec.jrn_',rec.table_name,' (
	      cdc_logposition  integer default nextval(''moto_mktg_allrec.transaction_log_position_seq'') not null,
				operation       char(1),
				trans_timestamp timestamp,
				like moto_mktg_allrec.',rec.table_name,'
			);');
	  
		execute moto_mktg_allrec.gen_cdc_function(rec.table_name);
	 
	end loop;
	
	
	for rec in select table_name from information_schema.tables where table_schema = 'moto_mktg' and table_name not like 'jrn_%'
	loop
	  --add fk's
	   for rec_fk in
        select oid, conname, replace(pg_get_constraintdef(oid),'moto_mktg','moto_mktg_allrec') as def
        from pg_constraint
        where contype = 'f'
        and conrelid = ('moto_mktg.'||rec.table_name)::regclass
    loop
        execute format(
            'alter table moto_mktg_allrec.%s add constraint %s %s',
            rec.table_name,
            rec_fk.conname,
            rec_fk.def);
    end loop;
	end loop;

end;
$$;


ALTER FUNCTION moto_mktg_allrec.build_moto_mktg_schema() OWNER TO amata;

--
-- Name: gen_cdc_function(character varying); Type: FUNCTION; Schema: moto_mktg_allrec; Owner: amata
--

CREATE FUNCTION moto_mktg_allrec.gen_cdc_function(ptable_name character varying) RETURNS text
    LANGUAGE plpgsql
    AS $_$  -- creates function for replaying changes from the cdc of moto_mktg on moto_mktg_allrec and populating the jrn table
declare
	cols          varchar[];
	keys          varchar[];
	coalesce_cols varchar[];
	update_cols   varchar[];
  attribute_cols varchar[];
begin
  
  select array_agg(column_name::varchar) into cols from information_schema.columns where table_schema = 'moto_mktg_allrec' and table_name = ptable_name;
  
  select array_agg(c.column_name::varchar) into keys
	FROM information_schema.table_constraints tc
	JOIN information_schema.constraint_column_usage AS ccu USING (constraint_schema, constraint_name)
	JOIN information_schema.columns AS c ON c.table_schema = tc.constraint_schema AND tc.table_name = c.table_name AND ccu.column_name = c.column_name
	WHERE constraint_type = 'PRIMARY KEY' and tc.table_name = ptable_name and tc.table_schema = 'moto_mktg_allrec';
  
  if keys is null then
	  select array_agg(c.column_name::varchar) into keys
		FROM information_schema.table_constraints tc
		JOIN information_schema.constraint_column_usage AS ccu USING (constraint_schema, constraint_name)
		JOIN information_schema.columns AS c ON c.table_schema = tc.constraint_schema AND tc.table_name = c.table_name AND ccu.column_name = c.column_name
		WHERE constraint_type = 'UNIQUE' and tc.table_name = ptable_name and tc.table_schema = 'moto_mktg_allrec';
  end if;
  
  select array_agg(element), array_agg(cc), array_agg(element||' = '||cc) into attribute_cols, coalesce_cols, update_cols
  from(
  select concat('coalesce(rec.'||element||', u.'||element||')') as cc, element
	from (
	  select unnest(cols)
	  except
	  select unnest(keys)
	) t (element))c;
 
	
	return concat(
'CREATE OR REPLACE FUNCTION moto_mktg_allrec.cdc_jrn_', ptable_name, '() returns void LANGUAGE plpgsql as
$func$
  declare
    rec record;
	begin
   
	  for rec in select * from  moto_mktg.jrn_',ptable_name,' where image_type is distinct from ''BEFORE'' order by trans_id
    loop
	   
		  if rec.operation = ''D'' then
	    
	    insert into moto_mktg_allrec.jrn_',ptable_name,'(operation, trans_timestamp, ',array_to_string(cols, ', '),')
	    select ''D'', rec.trans_timestamp, rec.',array_to_string(cols, ', rec.'),';
		  
		  delete from moto_mktg_allrec.',ptable_name,' d where (d.',array_to_string(keys, ', d.'),') = (rec.',array_to_string(keys, ', rec.'),');
		  
		  elsif rec.operation = ''I'' then
			  insert into moto_mktg_allrec.',ptable_name,'(',array_to_string(cols, ', '),')
			  select rec.', array_to_string(cols, ', rec.'),';
			  
			  insert into moto_mktg_allrec.jrn_',ptable_name,'(operation, trans_timestamp, ', array_to_string(cols, ', '),')
			  select
			    ''I'',
			    rec.trans_timestamp,
				  rec.', array_to_string(cols, ', rec.'),';
			  
		  else
		    
	      insert into moto_mktg_allrec.jrn_',ptable_name,'(operation, trans_timestamp, ',array_to_string(keys, ', '),', ', array_to_string(attribute_cols, ', '),')
			  select
			    ''U'',
			    rec.trans_timestamp,
				  rec.', array_to_string(keys, ', rec.'),',
				  ',array_to_string(coalesce_cols, ', '),'
	      from moto_mktg_allrec.',ptable_name,' u
	      where (u.',array_to_string(keys, ', u.'),') = (rec.',array_to_string(keys, ', rec.'),');
			
			  update moto_mktg_allrec.',ptable_name,' u
			  set
				  ',array_to_string(update_cols, ', '),'
			  where (u.',array_to_string(keys, ', u.'),') = (rec.',array_to_string(keys, ', rec.'),');
		  
		  end if;
				
		end loop;
	 
end;$func$;');

end;

$_$;


ALTER FUNCTION moto_mktg_allrec.gen_cdc_function(ptable_name character varying) OWNER TO amata;

